# Azure OpenAI
AZURE_OPENAI_API_KEY=your_openai_api_key_here
AZURE_OPENAI_RESOURCE_NAME=your_openai_resource_name_here
AZURE_OPENAI_API_VERSION=your_openai_api_version_here
ENABLE_LOGGING=true/false

# Session configuration
SESSION_SECRET=your_secure_session_secret_min_32_chars

# Access keys for different user groups (32 characters each)
ACCESS_KEY_GROUP_CHATBOT=1_xxxxxxxxxxxxxxxxxxxxxxxxxxxxx
ACCESS_KEY_GROUP_HUMAN_INTERVIEW=2_xxxxxxxxxxxxxxxxxxxxxxxxxxxxx
ACCESS_KEY_GROUP_TESTER=3_xxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Google Cloud
GCP_PROJECT_ID=your_project_id
GCP_BUCKET_NAME=your_bucket_name
# Copy the entire contents of your service account JSON key file and paste it as a single line
GCP_CREDENTIALS={"type": "service_account", "project_id": "...", "private_key": "...", "client_email": "..."}

# PostgreSQL Database Configuration
INSTANCE_CONNECTION_NAME=your_instance_connection_name
POSTGRES_DATABASE=your_database_name
POSTGRES_USER=your_username
POSTGRES_PASSWORD=your_password
